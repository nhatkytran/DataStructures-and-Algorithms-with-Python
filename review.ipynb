   "source": [
    "def sort(nums):\n",
    "    for i in range(1, len(nums)):\n",
    "        cur_num = nums[i]\n",
    "        while i - 1 >= 0 and nums[i - 1] > cur_num:\n",
    "            nums[i] = nums[i - 1]\n",
    "            i -= 1\n",
    "        nums[i] = cur_num\n",
    "    return nums\n",
    "\n",
    "\n",
    "print(sort([5, 3, 4, 7, 1, 2]))\n"
   ]
