    "def total(nums):\n",
    "    result = 0\n",
    "    for i in range(len(nums)):\n",
    "        for j in range(i + 1, len(nums)):\n",
    "            result += nums[i] ^ nums[j]\n",
    "    return result\n",
    "\n",
    "\n",
    "print(total([5,9,7,6]))\n"
   ]
