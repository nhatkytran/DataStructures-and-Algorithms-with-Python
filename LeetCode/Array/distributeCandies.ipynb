   "source": [
    "class Solution:\n",
    "    def distributeCandies(self, candyType):\n",
    "        canEat = len(candyType) / 2\n",
    "        candyType = set(candyType)\n",
    "        return min(int(canEat), len(candyType))\n",
    "    \n",
    "\n",
    "s = Solution()\n",
    "\n",
    "print(s.distributeCandies([1,1,2,2,3,3]))\n"
   ]
