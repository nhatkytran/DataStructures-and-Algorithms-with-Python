   "source": [
    "class Solution:\n",
    "    def smallestRangeI(self, nums, k):\n",
    "        max_value = max(nums)\n",
    "        min_value = min(nums)\n",
    "        diff = max_value - min_value\n",
    "        max_ = min(2 * k, diff)\n",
    "        return diff - max_\n",
    "\n",
    "\n",
    "s = Solution()\n",
    "\n",
    "print(s.smallestRangeI([1], k = 0))\n"
   ]
