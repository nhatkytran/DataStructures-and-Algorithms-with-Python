   "source": [
    "class Solution:\n",
    "    def findNumbers(self, nums):\n",
    "        from math import floor, log10\n",
    "        def get_digit(num):\n",
    "            return floor(log10(num)) + 1\n",
    "        return sum(1 if get_digit(num) % 2 == 0 else 0 for num in nums)\n",
    "\n",
    "\n",
    "s = Solution()\n",
    "\n",
    "print(s.findNumbers([12,345,2,6,7896]))\n"
   ]
