   "source": [
    "class Solution:\n",
    "    def separateDigits(self, nums):\n",
    "        result = []\n",
    "        for num in nums:\n",
    "            result.extend(int(n) for n in str(num))\n",
    "        return result\n",
    "\n",
    "\n",
    "s = Solution()\n",
    "\n",
    "print(s.separateDigits([13,25,83,77]))\n"
   ]
