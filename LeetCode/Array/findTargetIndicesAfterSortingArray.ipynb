    "class Solution:\n",
    "    def targetIndices(self, nums, target):\n",
    "        nums.sort()\n",
    "        result = []\n",
    "        for i in range(len(nums)):\n",
    "            if target == nums[i]:\n",
    "                result.append(i)\n",
    "        return result\n",
    "\n",
    "\n",
    "s = Solution()\n",
    "\n",
    "print(s.targetIndices([1,2,5,2,3], 2))\n"
   ]
