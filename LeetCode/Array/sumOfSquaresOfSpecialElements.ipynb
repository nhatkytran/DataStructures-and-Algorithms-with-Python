    "class Solution:\n",
    "    def sumOfSquares(self, nums):\n",
    "        n = len(nums)\n",
    "        result = 0\n",
    "        for i in range(1, n + 1):\n",
    "            if n % i == 0:\n",
    "                result += nums[i - 1] ** 2\n",
    "        return result\n",
    "\n",
    "\n",
    "s = Solution()\n",
    "\n",
    "print(s.sumOfSquares([1,2,3,4]))\n"
   ]
