   "source": [
    "class Solution:\n",
    "    def maximumCount(self, nums):\n",
    "        pos, neg = 0, 0\n",
    "        for num in nums:\n",
    "            if num > 0: pos += 1\n",
    "            if num < 0: neg += 1\n",
    "        return max(pos, neg)\n",
    "\n",
    "\n",
    "s = Solution()\n",
    "\n",
    "print(s.maximumCount([-2,-1,-1,1,2,3]))\n"
   ]
