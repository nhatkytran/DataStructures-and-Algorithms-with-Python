   "source": [
    "class Solution:\n",
    "    def countNegatives(self, grid):\n",
    "        return sum(1 for row in grid for number in row if number < 0)\n",
    "\n",
    "\n",
    "s = Solution()\n",
    "\n",
    "print(s.countNegatives([[4,3,2,-1],[3,2,1,-1],[1,1,-1,-2],[-1,-1,-2,-3]]))\n"
   ]
