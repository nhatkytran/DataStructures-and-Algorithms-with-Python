    "class Solution:\n",
    "    def maximizeSum(self, nums, k):\n",
    "        return max(nums) * k + sum(range(k))\n",
    "\n",
    "\n",
    "s = Solution()\n",
    "\n",
    "print(s.maximizeSum([1, 2, 3, 4, 5], 3))\n",
    "print(s.maximizeSum([5, 5, 5], 2))\n"
   ]
