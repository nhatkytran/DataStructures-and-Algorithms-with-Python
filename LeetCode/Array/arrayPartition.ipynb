    "class Solution:\n",
    "    def arrayPairSum(self, nums):\n",
    "        nums.sort()\n",
    "        return sum(nums[i] for i in range(0, len(nums), 2))\n",
    "\n",
    "\n",
    "s = Solution()\n",
    "\n",
    "print(s.arrayPairSum([1,4,3,2]))\n",
    "print(s.arrayPairSum([6,2,6,5,1,2]))\n"
   ]
