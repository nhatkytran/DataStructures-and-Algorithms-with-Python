    "def sum_curry(number_1):\n",
    "    def inner(number_2=None):\n",
    "        if not number_2:\n",
    "            return number_1\n",
    "        return sum_curry(number_1 + number_2)\n",
    "    return inner\n",
    "\n",
    "\n",
    "print(sum_curry(1)(2)(3)())\n"
   ]
